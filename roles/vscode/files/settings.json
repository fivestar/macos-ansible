{
    "workbench.colorTheme": "GitHub Dark",
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontFamily": "Hack, Menlo, monospace",
    "editor.rulers": [
        100
    ],
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "extensions.ignoreRecommendations": false,
    "alignment.surroundSpace": {
        "comment": 1
    },
    "git.confirmSync": false,
    "git.fetchOnPull": true,
    "go.formatTool": "goimports",
    "go.lintTool": "golangci-lint",
    "go.useLanguageServer": true,
    "go.autocompleteUnimportedPackages": true,
    "go.testOnSave": true,
    "go.toolsManagement.autoUpdate": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        }
    ],
    "redhat.telemetry.enabled": false,
    "search.useIgnoreFiles": false,
    "yaml.format.enable": false,
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "vsicons.dontShowNewVersionMessage": true,
    "editor.guides.bracketPairs": true,
    "trailing-spaces.trimOnSave": true,
    "peacock.elementAdjustments": {
        "activityBar": "lighten",
        "statusBar": "none",
        "titleBar": "none"
    },
    "protoc": {},
    "rust-analyzer.check.overrideCommand": null,
    "[proto3]": {
        "editor.formatOnSave": false
    },
    "editor.formatOnSave": true,
    "editor.fontSize": 13,
    "markdown.extension.tableFormatter.enabled": false,
    "git.openRepositoryInParentFolders": "never",
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path-mac": "${env:HOME}/.vs-kubernetes/tools/helm/darwin-arm64/helm",
        "vscode-kubernetes.minikube-path-mac": "/Users/fivestar/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
    },
    "grpc-federation": {
        "path": "${env:GOPATH}/src/github.com/kouzoh/merpay-grpc-federation/platform-proto/bin/grpc-federation-language-server",
        "import-paths": [
            "./proto"
        ]
    },
    "files.associations": {
        "*.md.txt": "markdown",
        "*.html": "liquid"
    },
    "makefile.configureOnOpen": false
}